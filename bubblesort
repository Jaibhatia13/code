#include<bits/stdc++.h>
using namespace std;
int main(){
    int arr[5]={21,23,45,76,29};
    for(int i=0 ; i<4;i++){
        for(int j=i ; j <4-i;j++){
        if(arr[j]>arr[j+1]){
            swap(arr[j],arr[j+1]);
        }
        }
    }
    for(int i =0 ;i<5;i++){
        cout<<arr[i]<<" ";
    }
}
